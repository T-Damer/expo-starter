{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "plugins": [
      {
        "name": "@0no-co/graphqlsp",
        "schema": "http://localhost:1337/graphql",
        "tadaOutputLocation": "./types/graphql-env.d.ts"
      }
    ]
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
